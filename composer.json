{
    "name": "inpsyde/logzio-monolog",
    "description": "Logz.io integration for Monolog",
    "keywords": [
        "log",
        "logging",
        "monolog"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Christian BrÃ¼ckner",
            "email": "chris@chrico.info",
            "homepage": "https://www.chrico.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "monolog/monolog": ">=3.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^8",
        "inpsyde/php-coding-standards": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Inpsyde\\LogzIoMonolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inpsyde\\LogzIoMonolog\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
